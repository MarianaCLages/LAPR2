@startuml
'https://plantuml.com/class-diagram

hide methods
skinparam classAttributeIconSize 0

class User{
-name
-email
-password
}

class Employee{
-Employee ID;
-Organization Role;
-Name;
-Address;
-Phone Number;
-E-Mail;
-Standard Occupational Classification "SOC";
}

class SpecialistDoctor{
-Doctor Index Number;
}

